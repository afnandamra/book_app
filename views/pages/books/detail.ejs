<section class="card">
    <img src='<%=book.img %>' alt="Book Cover">
    <h2>
        <%= book.title %>
    </h2>
    <h3>
        <%= book.author %>
    </h3>
    <p class="description">
        <%= book.description %>
    </p>
    <p>
        ISBN: <%= book.isbn %>
    </p>
    <p>
        Bookshelf: <%= book.shelf %>
    </p>
</section>